<div class="center">
    <% object.each do |category|%>
    <div class="row">
        <div class="col-xs-8 col-xs-offset-2">
            <div class="well well-lg">
                <div class="article-title">
                    <%= link_to category.name, category_path(category.id) %>
                </div>
            </div>
        </div>
    </div>
    <% end %>
</div>